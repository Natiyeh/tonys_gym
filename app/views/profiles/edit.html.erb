<%= simple_form_for [@user, @profile] do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :date_of_birth %>
  <%= f.input :profile_url %>
  <%= f.input :gender %>
  <%= f.input :street_address %>
  <%= f.input :postal_code %>
  <%= f.input :phone_number %>
  <%= f.button :submit, 'complete', class: 'btn btn-primary'%>
<% end %>
