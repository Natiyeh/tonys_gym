<div class="container">
  <div class="row">
    <div class="col-sm-10 col-sm-offset-4" style="padding:60px">
      <%= simple_form_for [@user, @profile] do |f| %>
        <%= f.input :first_name %>
        <%= f.input :last_name %>
        <%= f.input :profile_url, as: :file %>
        <%= f.input :date_of_birth %>
        <%= f.input :gender %>
        <%= f.input :street_address %>
        <%= f.input :state %>
        <%= f.input :city %>
        <%= f.input :postal_code %>
        <%= f.input :phone_number %>
        <%= f.button :submit, 'complete', class: 'btn btn-primary'%>
      <% end %>
    </div>
  </div>
</div>
